<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>All Skills</title>
	</head>
	<body>
		<h1>Skills</h1>
		<a href="/">Home</a>
		<ul>
			<% allSkills.forEach(function(s, id) { %>
			<li>
				<a href="/skills/<%= id %>"><%= s.title%></a>
			</li>
			<% }); %>
		<a href="/skills/new">Add New Skill</a>
		</ul>
	</body>
</html>
